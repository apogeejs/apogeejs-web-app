{
    "repoName": "apogeejs-web-app",

    "esModule": {
        "taskList": [
            {
                "type": "copyFileList",
                "srcFileUrlList": [
                    "/apogeejs-web-app/versionInfo.json"
                ]
            },
            {
                "type": "copyAndReplace",
                "srcFileUrl": "/apogeejs-web-app/web/apogee.html",
                "replacementList": [
                    {
                        "marker": "[RELEASE_VERSION]",
                        "lib": "this",
                        "type": "version"
                    },
                    {
                        "marker": "[APOGEE_BUNDLE_PATH]",
                        "lib": "apogeejs-app-bundle",
                        "type": "deployPath"
                    },
                    {
                        "marker": "[COMBINED_FILE_ACCESS_PATH]",
                        "lib": "apogeejs-combined-file-access",
                        "type": "deployPath"
                    },
                    {
                        "marker": "[RELEASE_PATH]",
                        "lib": "this",
                        "type": "deployPath"
                    }
                ]
            }
        ]
    }
}