{
    "repoName": "apogeejs-web-app",
    "taskList": [
        {
            "type": "copyAndReplace",
            "srcFileUrl": "/apogeejs-web-app/web/apogee.html",
            "replacementList": [
                {
                    "marker": "[RELEASE_VERSION]",
                    "lib": "this",
                    "type": "version"
                },
                {
                    "marker": "[DEPLOY_PATH]",
                    "lib": "this",
                    "type": "deployPath"
                }
            ],
            "childFolder": "web"
        },
        {
            "type": "concatFileList",
            "srcFileUrlList": [
                "/apogeejs-view-lib/style/**/*",
                "/apogeejs-app-bundle/style/**/*",
                "/apogeejs-ui-lib/style/**/*",
                "/prosemirror-admin/compiledCss/editor.css",    
                "/apogeejs-releases/releases/ext/handsontable/v6.2.0/handsontable.full.min.css"
            ],
            "destFileName": "apogeeAppBundle.css",
            "childFolder": "apogeejs-app-bundle/style"
        },
        {
            "type": "copyFileList",
            "srcFileUrlList": [
                "/apogeejs-ui-lib/resources/**/*",
                "/apogeejs-app-bundle/resources/**/*"
            ],
            "childFolder": "apogeejs-app-bundle/resources"
        },
        {
            "type": "copyFileList",
            "srcFileUrlList": [
                "/apogeejs-model-lib/src/debugHook.js",
                "/apogeejs-model-lib/src/webGlobals.js"
            ],
            "childFolder": "apogeejs-app-bundle/src"
        },
        {
            "type": "packageLib",
            "format": "es",
            "sourceFileUrl": "/apogeejs-app-bundle/src/apogeeAppBundle.js",
            "outputFileName": "apogeeAppBundle.js",
            "childFolder": "apogeejs-app-bundle/src",
            "externalLibMapping": {
                "/apogeejs-releases/releases/ext/esprima/v2.7.3/esprima.es.js": "/apogeejs-releases/releases/ext/esprima/v2.7.3/esprima.es.js",
                "/apogeejs-releases/releases/ext/ace/v1.4.3/ace.es.js": "/apogeejs-releases/releases/ext/ace/v1.4.3/ace.es.js",
                "/apogeejs-releases/releases/ext/handsontable/v6.2.0/handsontable.es.js": "/apogeejs-releases/releases/ext/handsontable/v6.2.0/handsontable.es.js",
                "/apogeejs-releases/releases/ext/prosemirror-model/v1.10.0/prosemirror-model.es.js": "/apogeejs-releases/releases/ext/prosemirror-model/v1.10.0/prosemirror-model.es.js",
                "/apogeejs-releases/releases/ext/prosemirror-state/v1.3.3/prosemirror-state.es.js": "/apogeejs-releases/releases/ext/prosemirror-state/v1.3.3/prosemirror-state.es.js",
                "/apogeejs-releases/releases/ext/prosemirror-transform/v1.2.6/prosemirror-transform.es.js": "/apogeejs-releases/releases/ext/prosemirror-transform/v1.2.6/prosemirror-transform.es.js",
                "/apogeejs-releases/releases/ext/prosemirror-view/v1.14.13/prosemirror-view.es.js": "/apogeejs-releases/releases/ext/prosemirror-view/v1.14.13/prosemirror-view.es.js",
                "/apogeejs-releases/releases/ext/prosemirror-keymap/v1.1.4/prosemirror-keymap.es.js": "/apogeejs-releases/releases/ext/prosemirror-keymap/v1.1.4/prosemirror-keymap.es.js",
                "/apogeejs-releases/releases/ext/prosemirror-commands/v1.1.4/prosemirror-commands.es.js": "/apogeejs-releases/releases/ext/prosemirror-commands/v1.1.4/prosemirror-commands.es.js",
                "/apogeejs-releases/releases/ext/prosemirror-gapcursor/v1.1.5/prosemirror-gapcursor.es.js": "/apogeejs-releases/releases/ext/prosemirror-gapcursor/v1.1.5/prosemirror-gapcursor.es.js"
            }
        },
        {
            "type": "copyFileList",
            "srcFileUrlList": [
                "/apogeejs-combined-file-access/style/**/*"
            ],
            "childFolder": "apogeejs-combined-file-access/style"
        },
        {
            "type": "copyFileList",
            "srcFileUrlList": [
                "/apogeejs-combined-file-access/resources/**/*"
            ],
            "childFolder": "apogeejs-combined-file-access/resources"
        },
        {
            "type": "packageLib",
            "format": "es",
            "sourceFileUrl": "/apogeejs-combined-file-access/src/CombinedFileAccessAppConfigManager.js",
            "outputFileName": "CombinedFileAccessAppConfigManager.js",
            "childFolder": "apogeejs-combined-file-access/src"
        }
    ]

}
