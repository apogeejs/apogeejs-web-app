{
    "repoName": "apogeejs-web-app",

    "esModule": {
        "taskList": [
            {
                "type": "copyFileList",
                "srcFileUrlList": [
                    "/apogeejs-web-app/versionInfo.json"
                ]
            },
            {
                "type": "copyAndReplace",
                "srcFileUrl": "/apogeejs-web-app/web/apogee.html",
                "replacementList": [
                    {
                        "marker": "[RELEASE_VERSION]",
                        "libVersion": "this"
                    },
                    {
                        "marker": "[APOGEE_BUNDLE_VERSION]",
                        "libVersion": "apogeejs-app-bundle"
                    },
                    {
                        "marker": "[COMBINED_FILE_ACCESS_VERSION]",
                        "libVersion": "apogeejs-combined-file-access"
                    },
                    {
                        "marker": "[BASE_HREF]",
                        "additionalValue": "BASE_HREF"
                    }
                ]
            }
        ]
    }
}